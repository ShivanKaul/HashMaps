{
  "manifest_version": 2,

  "name": "HashMaps",
  "short_name": "#",
  "description": "Adding Google Maps search to your address bar.",
  "version": "1.3.4",
  "homepage_url": "http://shivankaulsahib.me",
  "icons": {
      "64": "images/icon-64.png",
      "32": "images/icon-32.png",
      "128": "images/icon-128.png"
  },
  "permissions": [
          "storage"
        ],
  "browser_action": {
    "default_icon": "images/icon-16.png",
    "default_popup": "src/popup.html"
  },
  "background": {
      "scripts": [ "src/js/jquery-1.11.2.min.js", "src/js/background.js", "src/js/get-country-from-ip.js" ]
   },
  "omnibox": { "keyword" : "#" }
}
